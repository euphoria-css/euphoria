{
  "name": "euphoria",
  "version": "2.4.0",
  "description": "Minimalist CSS component and utility framework for CSS-in-JS applications",
  "main": "src/euphoria.js",
  "scripts": {
    "build-examples": "NODE_ENV=production webpack",
    "build-css": "node scripts/bundle.js",
    "build": "npm run build-examples && npm run build-css",
    "format": "prettier --write --ignore-path docs/main.js {src,scripts,docs}/**/*.js",
    "gzip": "gzip -9 -c dist/euphoria.min.css > dist/euphoria.min.css.gz",
    "start": "NODE_ENV=development webpack-dev-server",
    "test": "ava src/**/*.test.js",
    "watch-test": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/danawoodman/euphoria.git"
  },
  "keywords": [
    "css",
    "sass",
    "scss",
    "euphoria",
    "ui",
    "framework",
    "tool",
    "util",
    "utils",
    "utilties",
    "styling",
    "design",
    "bootstrap",
    "reset",
    "forms",
    "type",
    "tyopgraphy",
    "fonts",
    "font",
    "icon",
    "style"
  ],
  "author": "Dana Woodman <dana@danawoodman.com> (http://danawoodman.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danawoodman/euphoria/issues"
  },
  "homepage": "https://github.com/danawoodman/euphoria#readme",
  "devDependencies": {
    "ava": "0.24.0",
    "babel": "6.23.0",
    "babel-core": "6.25.0",
    "babel-loader": "7.0.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chalk": "2.3.0",
    "clean-css": "4.1.9",
    "extract-text-webpack-plugin": "2.1.2",
    "glamor": "2.20.25",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.28.0",
    "prettier": "1.9.2",
    "prop-types": "15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "4.2.2",
    "react-syntax-highlighter": "5.6.2",
    "stringify-object": "3.2.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "css-selector-parser": "1.3.0",
    "lightness": "1.0.0",
    "lodash": "4.17.4",
    "url-slug": "2.0.0"
  }
}
